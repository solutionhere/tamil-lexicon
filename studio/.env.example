# This file is an example.
# Copy this to a new file named .env.local to use these variables in development.
# IMPORTANT: .env.local should NOT be committed to Git.

# --- Firebase Configuration ---
# You can get these values from your Firebase project's settings page.
NEXT_PUBLIC_FIREBASE_API_KEY="your-api-key-here"
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN="your-auth-domain-here"
NEXT_PUBLIC_FIREBASE_PROJECT_ID="your-project-id-here"
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET="your-storage-bucket-here"
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID="your-sender-id-here"
NEXT_PUBLIC_FIREBASE_APP_ID="your-app-id-here"
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID="your-measurement-id-here"

# --- Google AI (Genkit) Configuration ---
# You can get this key from Google AI Studio.
GOOGLE_API_KEY="your-google-ai-api-key-here"
